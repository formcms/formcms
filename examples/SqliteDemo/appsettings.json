{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "mateStatic": {
        "ClusterId": "mate",
        "Match": {
          "Path": "/mate-static/{**catch-all}"
        }
      },
      "mateApi": {
        "ClusterId": "mate",
        "Match": {
          "Path": "/mateapi/{**catch-all}"
        }
      },
      "mateApp": {
        "ClusterId": "mate",
        "Match": {
          "Path": "/mate/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "mate": {
        "Destinations": {
          "destination": {
            "Address": "http://127.0.0.1:3001/"
          }
        }
      }
    }
  }
}
